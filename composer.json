{
  "name": "lambdacreatives/tofino",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "A WordPress starter theme for jumpstarting custom theme development.",
  "homepage": "https://github.com/lambdacreatives/tofino",
  "authors": [
    {
      "name": "Daniel Hewes",
      "email": "daniel@lambdacreatives.com",
      "homepage": "https://github.com/danimalweb"
    },
    {
      "name": "Jake Gully",
      "email": "chimpytk@gmail.com",
      "homepage": "https://github.com/mrchimp"
    }
  ],
  "keywords": [
    "WordPress",
    "theme",
    "tofino"
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "twittem/wp-bootstrap-navwalker",
        "version": "dev-master",
        "source": {
          "url": "https://github.com/twittem/wp-bootstrap-navwalker",
          "type": "git",
          "reference": "origin/master"
        }
      }
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "tgmpa/tgm-plugin-activation": "2.5.2",
    "twittem/wp-bootstrap-navwalker": "dev-master",
    "valendesigns/option-tree": "2.5.5",
    "google/recaptcha": "~1.1",
    "phpmailer/phpmailer": "~5.2"
  },
  "minimum-stability": "dev",
  "require-dev": {
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "files": [
      "vendor/twittem/wp-bootstrap-navwalker/wp_bootstrap_navwalker.php",
      "lib/init.php",
      "lib/assets.php",
      "lib/contact-form.php",
      "lib/required-plugins.php",
      "lib/shortcodes.php",
      "lib/theme-options.php",
      "lib/helpers.php"
    ]
  }
}
