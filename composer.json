{
  "name": "lambdacreatives/tofino",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "A WordPress starter theme for jumpstarting custom theme development.",
  "homepage": "https://github.com/lambdacreatives/tofino",
  "authors": [
    {
      "name": "Daniel Hewes",
      "email": "daniel@lambdacreatives.com",
      "homepage": "https://github.com/danimalweb"
    },
    {
      "name": "Jake Gully",
      "email": "chimpytk@gmail.com",
      "homepage": "https://github.com/mrchimp"
    }
  ],
  "keywords": [
    "WordPress",
    "theme",
    "tofino"
  ],
  "require": {
    "php": ">=5.4.0",
    "composer/installers": "~1.0",
    "google/recaptcha": "~1.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "2.5.0"
  },
  "autoload": {
    "files": [
      "src/lib/nav-walker.php",
      "src/lib/init.php",
      "src/lib/assets.php",
      "src/lib/helpers.php",
      "src/lib/relative-urls.php",
      "src/lib/theme-tracker.php",
      "src/forms/contact-form.php",
      "src/shortcodes.php",
      "src/theme-options.php"
    ],
    "psr-4": { "Tofino\\": "src/lib/" }
  }
}
